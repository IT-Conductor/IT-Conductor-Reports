<?xml version="1.0" encoding="UTF-8"?>
<itc:report xmlns:itc="urn:itconductor">
   <itc:parameter name="fontSize">4</itc:parameter>
   <h1>${self/REPORT.Param1} HANA CPU and Memory Utilization Report</h1>
   <pbr />
   <!-- this section -->
   <itc:section>
      <itc:parameter name="className">HANADb</itc:parameter>
      <itc:select name="APPLICATION_Role">${self/REPORT.Param1}</itc:select>
      <h2>${context/OBJECT_Name}</h2>
      <h3>CPU</h3>
      <itc:chart>
         <itc:parameter name="OBJECT_Id">6877493229506024</itc:parameter>
         <itc:parameter name="context">${context/OBJECT_Id}</itc:parameter>
         <itc:parameter name="table">false</itc:parameter>
         <itc:parameter name="timeScale">DAY</itc:parameter>
         <itc:parameter name="range">QTD</itc:parameter>
         <itc:parameter name="fit">width</itc:parameter>
         <itc:parameter name="legend">top</itc:parameter>
      </itc:chart>
      <h3>Memory</h3>
      <itc:chart>
         <itc:parameter name="OBJECT_Id">6839688410740725</itc:parameter>
         <itc:parameter name="context">${context/OBJECT_Id}</itc:parameter>
         <itc:parameter name="table">false</itc:parameter>
         <itc:parameter name="timeScale">DAY</itc:parameter>
         <itc:parameter name="range">QTD</itc:parameter>
         <itc:parameter name="fit">width</itc:parameter>
         <itc:parameter name="legend">top</itc:parameter>
      </itc:chart>
